@model PortfolioCoreDay.Entities.Contact

@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>İletişim Sayfası Güncelleme Girişi</h1>

<form method="post">
    <input type="hidden" asp-for="ContactId" />
    <label>Adres</label>
    <input type="text" asp-for="Address" class="form-control" style="border:1px solid black" />
    <br />
    <label>E-Mail</label>
    <input type="text" asp-for="Email" class="form-control" style="border:1px solid black" />
    <br />
    <label>Telefon</label>
    <input type="text" asp-for="Number" class="form-control" style="border:1px solid black" />
    <br />
    <label>Harita URL</label>
    <input type="text" asp-for="MapUrl" class="form-control" style="border:1px solid black" />
    <br />
    <button class="btn btn-primary">Güncelle</button>
</form>